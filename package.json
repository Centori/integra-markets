{
  "name": "integra",
  "version": "1.0.0",
  "type": "commonjs",
  "private": true,
  "description": "AI-powered commodity trading insights app",
  "main": "index.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@expo/cli": "^0.24.20",
    "@expo/metro-runtime": "^4.0.1",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "buffer": "^6.0.3",
    "expo": "^52.0.47",
    "expo-auth-session": "~6.0.3",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-document-picker": "~13.0.3",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-notifications": "~0.29.14",
    "expo-speech": "~13.0.1",
    "expo-updates": "^0.27.4",
    "expo-web-browser": "~14.0.2",
    "lucide-react-native": "^0.525.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "^15.8.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "~0.19.13",
    "react-native-chart-kit": "^6.12.0"
  },
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev": "npx expo start",
    "start": "npx expo start",
    "web": "npx expo start --web",
    "build:web": "npx expo export --platform web --output-dir dist",
    "build:web:prod": "NODE_ENV=production npx expo export --platform web --output-dir dist --clear",
    "deploy:vercel": "npm run build:web:prod && vercel --prod",
    "preview:web": "npm run build:web && npx serve dist",
    "deploy": "npx railway up --service integra-markets",
    "deploy:logs": "npx railway logs --service integra-markets",
    "deploy:status": "npx railway status",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "test": "jest",
    "test:testsprite": "node __tests__/testsprite-runner.js",
    "test:frontend": "node __tests__/testsprite-runner.js frontend",
    "test:api": "node __tests__/testsprite-runner.js api",
    "test:auth": "node __tests__/testsprite-runner.js auth",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^19.1.1",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "expo-dev-client": "~5.0.20",
    "jest": "^29.0.0",
    "jest-expo": "~52.0.6",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3"
  },
  "license": "MIT"
}
