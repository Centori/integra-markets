{
  "expo": {
    "name": "Integra Markets",
    "slug": "integra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "description": "Professional financial markets analysis and news aggregation platform with AI-powered sentiment analysis for commodities trading.",
    "keywords": ["finance", "markets", "trading", "commodities", "news", "analysis", "AI", "sentiment"],
    "category": "finance",
    "privacy": "https://integramarkets.com/privacy",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.centori.integramarkets",
      "buildNumber": "27",
      "deploymentTarget": "13.0",
      "supportsTablet": true,
      "requireFullScreen": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "appStoreUrl": "https://apps.apple.com/app/integra-markets/id123456789",
      "category": "finance",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to capture financial documents and receipts.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select financial documents and receipts.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for voice messages and audio analysis.",
        "NSFaceIDUsageDescription": "This app uses Face ID for secure authentication.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": false,
          "NSExceptionDomains": {}
        },
        "NSUserNotificationsUsageDescription": "This app sends notifications for market alerts and breaking news.",
        "NSKeychainAccessGroups": [
          "$(AppIdentifierPrefix)com.centori.integramarkets"
        ],
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "UIRequiresFullScreen": false,
        "LSRequiresIPhoneOS": true,
        "UIStatusBarStyle": "UIStatusBarStyleDefault",
        "UNUserNotificationCenter": {
          "UNAuthorizationOptionAlert": true,
          "UNAuthorizationOptionBadge": true,
          "UNAuthorizationOptionSound": true,
          "UNAuthorizationOptionCriticalAlert": false,
          "UNAuthorizationOptionProvidesAppNotificationSettings": true
        }
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.centori.integramarkets",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_FINGERPRINT"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "baseUrl": "/"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4ECCA3",
          "defaultChannel": "default"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1039046627332-nk0jejccajfd9u63p5kas0l5ps53nlsq"
        }
      ]
    ],
    "extra": {
      "supabaseUrl": "https://jcovjmuaysebdfbpbvdh.supabase.co",
      "supabaseAnonKey": "__SUPABASE_ANON_KEY__",
      "apiUrl": "__API_URL__",
      "groqApiKey": "__GROQ_API_KEY__",
      "huggingFaceToken": "__HUGGING_FACE_TOKEN__",
      "eas": {
        "projectId": "e9868cd6-adaa-422c-be2b-89b6f028e5f6"
      }
    },
    "newArchEnabled": false,
    "owner": "ak88"
  }
}
